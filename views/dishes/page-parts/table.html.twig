<div class="container">
    <table class="table table-dark table-bordered">
        <thead>
        <tr>
            <th>NAME</th>
            <th>COMPLEXITY</th>
            <th>TIME</th>
            <th># INGREDIENTS</th>
        </tr>
        </thead>
        <tbody>
        {% for dish in dishes %}
            <tr>
                <td>{{ dish.name }}</td>
                <td>{{ dish.complexityName }}</td>
                <td>{{ dish.time }}</td>
                <td class="text-right">{{ dish.numberOfIngredients }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
</div>